{
  "manifest_version": 3,
  "name": "Chrome extension BoilerPlate",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "options_page": "src/optionsPage/optionsPage.html",
  "content_scripts": [
    {
      "js": [
        "assets/content.tsx-loader-6cf90737.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "assets/content-12f8ccd1.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/hash-90858e81.js",
        "assets/content.tsx-ff54581a.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
